<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This job adds rpm packages to Zato repository in two steps:&#xd;
&#xd;
1. It uses &apos;rsync&apos; command to syncronize the local Zato repository with official, remote Zato repository on CentOS.&#xd;
&#xd;
2. It uses &apos;createrepo --update&quot; command to update remote Zato repository.</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>echo ${ZATO_LOCAL_REPO_BASE_DIR}/${SUITE}/${OPERATING_SYSTEM} ${CODENAME} ${META_INSTALLER_BASE_DIR}/${DISTRIBUTION}/zato-${RELEASE_VERSION}-${PACKAGE_NUMBER}_${ARCHITECTURE}-${CODENAME}.rpm

echo &quot;zato-add-rpm-packages-to-repo HAS BEEN TRIGGERED!&quot;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>